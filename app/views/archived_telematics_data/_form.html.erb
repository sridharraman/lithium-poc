<%= form_for @archived_telematics_datum, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="form-group">
    <%= f.label :vehicle_id, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :vehicle_id, :class => 'text_field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :vin_number, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :vin_number, :class => 'text_field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :gps_date_time, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :gps_date_time, :class => 'text_field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :lat, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :lat, :class => 'text_field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :lon, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :lon, :class => 'text_field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :direction, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :direction, :class => 'text_field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :gps_speed, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :gps_speed, :class => 'text_field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :soc, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :soc, :class => 'text_field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :dte, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :dte, :class => 'text_field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :odometer, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :odometer, :class => 'text_field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :car_status, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :car_status, :class => 'text_field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :hvac_status, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :hvac_status, :class => 'text_field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :server_time, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :server_time, :class => 'text_field form-control' %>
    </div>
  </div>

  <div class="form-group">
    <div class='col-md-offset-2 col-md-10'>
      <%= f.submit nil, :class => 'btn btn-primary' %>
      <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                  archived_telematics_data_path, :class => 'btn btn-default' %>
    </div>
  </div>
<% end %>
